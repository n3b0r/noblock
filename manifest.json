{
	"name": "Don't stop me now",
	"version": "1.0",
	"description": "Disable blockers into media webpages",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["popup.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"16": "images/get_started16.png",
		"32": "images/get_started32.png",
		"48": "images/get_started48.png",
		"128": "images/get_started128.png"
	},
	"manifest_version": 2
}
